{
  "name": "vox-cli",
  "version": "1.0.0",
  "description": "微风平台构建工具",
  "private": true,
  "bin": {
    "vox": "./bin/cli.js"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint --ext .js --fix ./lib",
    "tex": "node ./bin/cli.js --init",
    "dll": "node ./bin/cli.js dll",
    "vox": "node ./bin/cli.js build"
  },
  "files": [
    "bin",
    "boilerplates",
    "index.js",
    "lib",
    "package.json",
    ".gitignore",
    ".commitlintrc.js",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc.js",
    ".gitattributes",
    ".prettierrc.js",
    "jest.config.js",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "cross-env commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
