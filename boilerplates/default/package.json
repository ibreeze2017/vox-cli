{
  "name": "vox-demo",
  "version": "1.0.0",
  "description": "微风平台构建工具",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development vox --mode dev",
    "build": "cross-env NODE_ENV=production vox --mode build",
    "build:ans": "cross-env NODE_ENV=production vox --mode analysis",
    "build:dll": "cross-env NODE_ENV=production vox --mode dll",
    "deploy": "cross-env NODE_ENV=production vox --mode deploy",
    "test": "jest --coverage",
    "lint:ts": "eslint --ext .tsx,.ts --fix ./src",
    "lint:css": "stylelint src/**/*.css src/**/*.less src/**/*.scss --fix",
    "lint": "npm run lint:ts && npm run lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "cross-env commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
